  },
  "homepage": "https://github.com/graycraft/me#readme",
  "engines": {
    "node": "22.11.0",
    "npm": "10.9.0"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.21.1",
